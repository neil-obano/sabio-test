{
  "name": "expressonly",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "rimraf dist/*",
    "tsc": "tsc",
    "lint": "npx eslint . --ext .ts,.tsx",
    "start": "node -r dotenv/config ./dist/server.js",
    "build": "npm-run-all clean lint tsc",
    "dev:start": "npm-run-all build start",
    "dev": "nodemon --watch src -e ts --exec npm run dev:start",
    "test": "jest"
  },
  "dependencies": {
    "@types/cors": "^2.8.10",
    "axios": "^0.21.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-validator": "^6.10.0",
    "morgan": "~1.9.1",
    "passport": "^0.4.1",
    "passport-http": "^0.3.0",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/debug": "^4.1.5",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/morgan": "^1.9.2",
    "@types/passport": "^1.0.6",
    "@types/passport-http": "^0.3.8",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "eslint": "^7.21.0",
    "eslint-plugin-jest": "^24.1.5",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.2",
    "typescript": "^4.2.2"
  }
}
